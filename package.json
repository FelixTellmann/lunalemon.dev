{
  "name": "flex",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel next:dev watch:sections",
    "next:dev": "SET NEXT_PUBLIC_PORT=3000&& rimraf .next && next --port 3000",
    "watch:sections": "SET NEXT_PUBLIC_PORT=3000&& node scripts/watch-scripts.js",
    "build": "rimraf .next && next build",
    "build:start": "rimraf .next && next build --profile && next start",
    "start": "next start",
    "export": "next build && next export -o .export",
    "shopify": "shopify-ftp",
    "codegen": "graphql-codegen --config .graphql.yml"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
