    "@trpc/client": "^9.23.3",
    "@trpc/next": "^9.23.3",
    "@trpc/react": "^9.23.3",
    "@trpc/server": "^9.23.3",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.5.0",
    "jsdom": "^19.0.0",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "next-themes": "0.2.0",
    "raw-body": "^2.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.39.0",
